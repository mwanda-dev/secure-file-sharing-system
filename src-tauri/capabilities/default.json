{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "core:event:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "store:allow-delete",
    "store:allow-load",
    "store:allow-save",
    "store:allow-get",
    "store:allow-set",
    "store:allow-length",
    "clipboard-manager:allow-read-text",
    "clipboard-manager:allow-write-text"
  ]
}